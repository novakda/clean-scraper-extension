{
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.vue", "*.ts"],
      "options": {
        "parser": "vue"
      }
    },
    {
      "files": ["*.json", "*.md"],
      "options": {
        "parser": "markdown"
      }
    }
  ]
}
